  "private": true,
  "scripts": {
    "build": "tsc --noEmit && nest build",
    "env": "sync-dotenv",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "nest:generate:resource": "nest g resource",
    "nest:upgrade": "npx yarn-upgrade-all --ignore '^((?!@nestjs).)*$' --upgrade",
    "prebuild": "rimraf dist",
    "start": "nest start -e 'node -r newrelic'",
    "start:debug": "nest start --debug --watch -e 'node -r newrelic'",
    "start:dev": "rimraf dist && nest start --watch",
    "start:prod": "node dist/main",
    "start:prod:in-docker": "pnpm --dir ./node_modules/@altairgraphql/db prod:prestart && node dist/main",
    "migrate:e2e": "dotenv -e .env.e2e -- pnpm dlx prisma@5.16.2 migrate deploy --schema ../altair-db/prisma/schema.prisma",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e .env.e2e -- jest --config ./test/jest-e2e.config.js --detectOpenHandles --forceExit",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/altair-graphql/altair.git"
  }
