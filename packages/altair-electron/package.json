  "license": "MIT",
  "main": "dist/index.js",
  "private": true,
  "productName": "Altair GraphQL Client",
  "repository": {
    "type": "git",
    "url": "https://github.com/altair-graphql/altair.git"
  },
  "scripts": {
    "build": "pnpm compile && electron-builder",
    "compile": "tsc && pnpm sentry:sourcemaps:inject",
    "dev": "pnpm compile && electron ./dist/",
    "bootstrap": "pnpm compile",
    "postinstall": "electron-builder install-app-deps",
    "test": "pnpm test:unit",
    "test:e2e": "([[ -n $RUNNER_OS ]] && [[ $RUNNER_OS != 'Linux' ]]) && exit 0 || playwright test e2e",
    "test:e2e:old": "jest -c jest.config.e2e.js --runInBand",
    "test:unit": "jest",
    "sentry:sourcemaps:inject": "sentry-cli sourcemaps inject --org altair-graphql --project electron ./dist",
    "sentry:sourcemaps:upload": "sentry-cli sourcemaps upload --org altair-graphql --project electron ./dist",
    "sentry:sourcemaps": "pnpm sentry:sourcemaps:inject && pnpm sentry:sourcemaps:upload"
  }
}
