    "test": "jest && ng lint",
    "test:watch": "jest --watch",
    "prepare": "yarn generate-settings-schema-validator && node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --outputHashing=none --prod --aot --stats-json",
