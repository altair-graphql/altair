    "tslint": "^5.9.1",
    "typescript": ">=3.1.1 <3.2",
    "typescript-json-schema": "^0.38.0",
    "web-ext": "^3.0.0"
  },
  "config": {
    "forge": "./forge.config.js"
  },
  "repository": {
