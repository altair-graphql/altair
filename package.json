    "@clr/angular": "^2.0.2",
    "@clr/icons": "^2.0.2",
    "@clr/ui": "^2.0.2",
    "@ctrl/ngx-codemirror": "^2.0.0",
    "@ngrx/effects": "^8.1.0",
