{
  "name": "altair",
  "version": "1.4.0",
  "license": "MIT",
  "author": "samuelimolo4real@gmail.com",
  "description": "The best graphQL client you will ever need",
  "homepage": "https://imolorhe.github.io/altair/",
  "keywords": ["graphql", "altair", "wysiwyg", "editor", "client"],
  "main": "electron/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "node server",
    "build": "ng build -prod --aot",
    "test": "ng test",
    "test-build": "ng lint && npm run build && karma start karma.conf.js --single-run",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "npm run build",
    "electron": "npm run build && ./electron/fix-base-path.sh && ./node_modules/.bin/electron .",
    "build-electron": "./bin/package_electron_app.sh",
    "build-ext": "npm run build && ./bin/chrome_ext.sh",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "engines": {
    "node": "6.10.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.3.1",
    "@angular/cli": "^1.2.4",
    "@angular/common": "^4.3.1",
    "@angular/compiler": "^4.3.1",
    "@angular/compiler-cli": "^4.3.1",
    "@angular/core": "^4.3.1",
    "@angular/forms": "^4.3.1",
    "@angular/http": "^4.3.1",
    "@angular/platform-browser": "^4.3.1",
    "@angular/platform-browser-dynamic": "^4.3.1",
    "@angular/router": "^4.3.1",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.4",
    "@ngrx/store": "^2.2.3",
    "@ngrx/store-devtools": "^3.2.4",
    "body-parser": "^1.17.1",
    "codemirror": "^5.25.0",
    "codemirror-graphql": "^0.6.11",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "electron-compile": "^6.4.1",
    "express": "^4.15.2",
    "graphql": "^0.10.5",
    "marked": "^0.3.6",
    "ng2-codemirror": "^1.1.2",
    "ngrx-store-localstorage": "^0.1.8",
    "rxjs": "^5.4.2",
    "uuid": "^3.0.1",
    "valid-url": "^1.0.9",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/electron": "^1.4.38",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "codelyzer": "~2.0.0",
    "electron-prebuilt-compile": "1.6.11",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mime-types": "^2.1.15",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "asar": true,
        "quiet": true,
        "protocol": "altair://",
        "protocolName": "Altair GraphQL Client File",
        "icon": "./electron/logo",
        "prune": true,
        "ignore": [
          "idea",
          "bin",
          "chrome-ext-files",
          "docs",
          "e2e"
        ]
      },
      "electronWinstallerConfig": {
        "name": "Altair",
        "setupIcon": "./electron/logo.ico"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "imolorhe",
        "name": "altair"
      },
      "windowsStoreConfig": {
        "packageName": "Altair GraphQL Client"
      }
    }
  }
}
