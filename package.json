    "opencollective": "^1.0.3",
    "popper.js": "^1.15.0",
    "prettier": "^1.18.2",
    "rxjs": "^6.5.2",
    "snyk": "^1.195.0",
    "sortablejs": "^1.10.0-rc3",
    "subscriptions-transport-ws": "^0.9.16",
    "sval": "^0.4.6",
    "to-snake-case": "^1.0.0",
