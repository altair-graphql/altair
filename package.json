    "spectron": "6.0.0",
    "start-server-and-test": "^1.9.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.4.5",
    "typescript-json-schema": "^0.39.0",
    "web-ext": "^3.1.1"
  },
  "config": {
