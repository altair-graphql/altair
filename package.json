    "electron-debug": "^3.0.0",
    "electron-is-dev": "^1.0.0",
    "electron-log": "^3.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^4.0.1",
    "electron-util": "^0.10.0",
    "electron-window-state": "^5.0.3",
    "express": "^4.16.2",
    "file-dialog": "^0.0.7",
    "file-saver": "^2.0.0-rc.4",
    "graphql": "^14.0.2",
    "graphql-query-compress": "^1.0.0",
    "marked": "^0.6.2",
    "mousetrap": "^1.6.2",
