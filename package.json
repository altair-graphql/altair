    "codelyzer": "^4.0.2",
    "devtron": "^1.4.0",
    "electron-builder": "^20.27.1",
    "electron-chromedriver": "^5.0.0-beta.1",
    "electron-prebuilt-compile": "^4.0.0",
    "electron-reloader": "^0.3.0",
    "jasmine-core": "^3.1.0",
