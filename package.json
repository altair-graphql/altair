    "@clr/ui": "^0.13.7",
    "@ctrl/ngx-codemirror": "^2.0.0",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "@ngrx/store-devtools": "^6.1.0",
