    "electron-log": "^3.0.6",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^4.1.2",
    "electron-util": "^0.12.1",
    "electron-window-state": "^5.0.3",
    "express": "^4.17.1",
    "file-dialog": "^0.0.7",
    "file-saver": "^2.0.2",
    "graphql": "^14.4.2",
    "graphql-query-compress": "^1.2.2",
    "marked": "^0.7.0",
    "mousetrap": "^1.6.3",
