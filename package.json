    "rxjs": "^6.3.3",
    "sandboxr": "^0.16.2",
    "sortablejs": "^1.7.0",
    "subscriptions-transport-ws": "0.9.16",
    "to-snake-case": "^1.0.0",
    "uuid": "^3.1.0",
    "valid-url": "^1.0.9",
